      placeholder="Select an option"
      showClear={true}
      showSelectionIndicator={true}
      value="{{ tenant_subdomain.value }}"
      values="{{ item.subdomain }}"
    >
      <Event
        event="change"
        method="run"
        params={{
          map: {
            src: 'localStorage.setValue(\n  "SelectedTenant",\n  ClienteDropDown.selectedItem);\nSelectedTenant.setValue(ClienteDropDown.selectedItem);\ntenant_subdomain.setValue(ClienteDropDown.selectedItem.subdomain);',
          },
        }}
        pluginId=""
        type="script"
        waitMs="0"
        waitType="debounce"
      />
    </Select>
    <Alert2
      id="alert1"
      value={
        '{{ embedding_missing_counts.data.total_missing>0 ? "warning" : "success" }}'
      }
    >
      <State
        id="0"
        actionIconPositions="right"
