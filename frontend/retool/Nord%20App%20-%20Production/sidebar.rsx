          />
        </FileButton>
        <Button id="button36" iconBefore="bold/interface-download-button-2">
          <Event
            event="click"
            method="run"
            params={{
              map: {
                src: 'bulk_import_template.trigger({\n  onSuccess: (data) => {\n    const base64 = data?.base64Data;\n    if (!base64 || typeof base64 !== "string" || base64.length < 10) {\n      utils.showNotification({\n        title: "Invalid file data",\n        description: "No base64 string found in response.",\n        intent: "danger"\n      });\n      return;\n    }\n\n    const clean = base64.includes(",") ? base64.split(",").pop() : base64;\n    const byteChars = atob(clean);\n    const bytes = new Uint8Array(byteChars.length);\n    for (let i = 0; i < byteChars.length; i++) bytes[i] = byteChars.charCodeAt(i);\n\n    const blob = new Blob([bytes], {\n      type: "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"\n    });\n    utils.downloadFile(blob, "bulk_import_template.xlsx");\n  }\n});\n',
              },
            }}
            pluginId=""
            type="script"
            waitMs="0"
            waitType="debounce"
          />
        </Button>
        <ButtonGroup2 id="buttonGroup8" alignment="right" overflowPosition={1}>
          <ButtonGroup2-Button
            id="e1f05"
            icon="bold/interface-add-1"
            iconPosition="left"
            styleVariant="outline"
