        iconPositions="center"
        icons="bold/interface-delete-circle"
        styleVariants="error"
        titles="Error"
        tooltips={null}
        values="error"
      />
      <Event
        event="click"
        method="trigger"
        params={{}}
        pluginId="embeddings_backfill"
        type="datasource"
        waitMs="0"
        waitType="debounce"
      />
